{
  "name": "automated-polymorpher",
  "title": "Automated Evocations - Polymorpher Manager",
  "description": "A user interface to manage polymorpher with summoning animations and automated summoning for spells",
  "version": "1.0.0",
  "author": "theRipper93, p4535992",
  "type": "module",
  "socket": true,
  "includes": [
    "./assets/**",
    "./lang/**",
    "./scripts/**",
    "./styles/**",
    "./templates/**",
    "./module.json",
    "./README.md",
    "./icons/**",
    "./packs/**"
  ],
  "media": [
    {
      "type": "icon",
      "location": ""
    },
    {
      "type": "cover",
      "location": ""
    },
    {
      "type": "screenshot",
      "location": ""
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "packs": [
    {
      "name": "automated-polymorpher-macros",
      "label": "Automated Polymorpher Macros",
      "path": "packs/macros.db",
      "entity": "Macro",
      "module": "automated-polymorpher"
    },
    {
      "name": "automated-polymorpher-actors",
      "label": "Automated Polymorpher Summons",
      "path": "packs/actors.db",
      "entity": "Actor",
      "module": "automated-polymorpher"
    }
  ],
  "systems": ["dnd5e"],
  "scripts": [],
  "esmodules": ["automated-polymorpher.js"],
  "styles": ["styles/polymorphermanager.css"],
  "url": "https://github.com/p4535992/foundryvtt-automated-polymorpher",
  "manifest": "https://raw.githubusercontent.com/p4535992/foundryvtt-automated-polymorpher/main/src/module.json",
  "download": "https://raw.githubusercontent.com/p4535992/foundryvtt-automated-polymorpher/main/package/foundryvtt-automated-polymorpher-v1.0.0.zip",
  "minimumCoreVersion": "0.8.9",
  "compatibleCoreVersion": "0.8.9",
  "manifestPlusVersion": "1.2.1",
  "changelog": "https://raw.githubusercontent.com/p4535992/foundryvtt-automated-polymorpher/main/changelog.md",
  "bugs": "https://github.com/p4535992/foundryvtt-automated-polymorpher/issues",
  "allowBugReporter": true,
  "dependencies": [
    {
      "name": "lib-wrapper"
    },
    {
      "name": "sequencer"
    }
  ]
